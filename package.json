{
  "name": "delivery-tax",
  "version": "1.0.0",
  "description": "Riderize's delivery tax service",
  "main": "server.js",
  "config": {
    "s3BucketName": "delivery-tax-bucket",
    "region": "us-east-1",
    "cloudFormationStackName": "DeliveryTaxApi",
    "domain": ""
  },
  "scripts": {
    "build": "npx tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect 9300 --transpile-only src/server.ts",
    "dev:queue": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/providers/cron_jobs",
    "package": "cross-var aws cloudformation package --template ./sam-template.yml --s3-bucket $npm_package_config_s3BucketName --output-template sam-template.packaged.yml --region $npm_package_config_region",
    "deploy": "cross-var aws cloudformation deploy --template-file sam-template.packaged.yml --stack-name $npm_package_config_cloudFormationStackName --capabilities CAPABILITY_IAM --region $npm_package_config_region --parameter-overrides DomainName=$npm_package_config_domain",
    "package-deploy": "npm run package && npm run deploy",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Riderize/delivery-tax.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Riderize/delivery-tax/issues"
  },
  "homepage": "https://github.com/Riderize/delivery-tax#readme",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.26.4",
    "@types/node": "^15.0.2",
    "@types/pg": "^8.6.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "cross-var": "^1.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.3.11",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "@shared/": "./build/shared"
  },
  "packageManager": "yarn@1.22.19"
}
